SFX OL.KELISHIK("KL")
	
	[ENDSWITH "[aeiou]"]
    TSH = "ni"
    QRT = "ning"
    O'P = "nda"
    CHQ = "ndan"
    O'PH = "ndagi"
    O'PHD = "ndagidek"
	JNL = "nga"
